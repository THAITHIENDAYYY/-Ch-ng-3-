#include <iostream>
#include <algorithm>
 using namespace std;
 
int main(){
	int n=0; 
	int a[100];
	
	cout << " nhap cac phan tu ( nhap 0 de dung): " << endl; 
	do {
	    cin >> a[n];
	    n++;
	} while (a[n-1] !=0 );
	
	sort (a, a+n);
	cout << "mang sau khi sap xep tang dan " << endl;	
	for (int i = 0; i < n; i++){
		cout << a[i] << "" ;
		}

	cout <<endl;
	return 0;
	}
